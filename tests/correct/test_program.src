program test_program is
    int fibb_result;
    procedure fibb(int n in, int result out)
        int a;
        int b;
    begin
        if (n < 0) then
            result := -1;
            return;
        end if;
        if (n == 0) then
            result := 0;
            return;
        end if;
        if (n == 1) then
            result := 1;
            return;
        end if;
        
        fibb(n-1, a);
        fibb(n-2, b);

        result := a + b;
    end procedure;
begin
    fibb_result := -1234;
    fibb(12, fibb_result);
    
end program